            v1571 = function()
                v11.BankTitle.Owner.Text = "@" .. v7.Functions.UserIdToUsername(v23.Owner) or "???"
            end
            (v1570(v1571))()
            v11.BankTitle.Visible = true
            v11.Title.Visible = false
            return 
        end
    end
    v1590 = v11
    v1591 = v1590.BankTitle
    v1590 = false
    v1591.Visible = v1590
    v1590 = v11
    v1591 = v1590.Title
    v1590 = true
    v1591.Visible = v1590
end
