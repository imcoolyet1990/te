BankScrolling = function()
    local v1347 = v11.Container.Bank.Pets
    local v1348 = v1347:FindFirstChildOfClass("UIGridLayout")
    local v1350 = v1347:FindFirstChildOfClass("UIPadding")
    local v1354 = v7.Functions.ResolutionScale()
    local v1382
    local v1383 = 0.5
    if v1383 >= v1354 then
        v1382 = 3
    else
        v1383 = 1
        if v1383 >= v1354 then
            v1382 = 4
        else
            v1383 = 1.3
            if v1383 >= v1354 then
                v1382 = 5
            else
                v1382 = 6
            end
        end
    end
    v1383 = v33
    if not v1383 then
        v1383 = UDim2.new
        v1348.CellPadding = v1383(0, 10, 0, 10)
    else
        v1359 = UDim2.new
        v1348.CellPadding = v1359(0, 20, 0, 20)
    end
    v1364 = v33
    if not v1364 then
        v1382 = v1382 * 2
    end
    v1364 = v1347.AbsoluteSize.X - v1347.ScrollBarThickness - v1350.PaddingLeft.Offset - v1350.PaddingRight.Offset / v1382 - v1348.CellPadding.X.Offset
    v1348.CellSize = UDim2.new(0, v1364, 0, v1364)
    v1348:ApplyLayout()
    local v1377 = v1348.AbsoluteContentSize
    if not v33 then
        v1377 = 55
    else
        v1377 = 65
    end
    v1347.CanvasSize = UDim2.new(1, 0, 0, v1377.Y + v1377)
end
