            table.remove(p10, v1045)
            return 
        end
    end
    v1030 = #p10
    v1045 = v7
    v1032 = v1045.Shared
    v1031 = v1032.BankMaxPetsPerTransaction
    if v1031 > v1030 then
        v1032 = v7
        v1031 = v1032.Audio
        v1030 = v1031.Play
        v1031 = "rbxassetid://7139317934"
        v1032 = script
        v1045 = v15
        v1033 = 0.75
        v1030(v1031, v1032, v1045:NextNumber(0.95, 1.05), v1033)
        table.insert(p10, p11)
    end
end
